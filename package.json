{
  "name": "replicache-sk",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "start:wrangler": "wrangler pages dev .svelte-kit/cloudflare --compatibility-date=2024-01-02 --live-reload",
    "start:watch": "bun run build -- -w",
    "drizzle:migrate": "bun drizzle/migrate.ts",
    "drizzle:generate": "drizzle-kit generate:sqlite"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-cloudflare": "^4.0.1",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "drizzle-kit": "^0.20.13",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vitest": "^1.2.0",
    "wrangler": "^3.22.5"
  },
  "type": "module",
  "dependencies": {
    "@libsql/client": "^0.4.0-pre.7",
    "drizzle-orm": "^0.29.3",
    "drizzle-zod": "^0.5.1",
    "nanoid": "^5.0.4",
    "pusher": "^5.2.0",
    "pusher-js": "^8.4.0-rc2",
    "replicache": "^14.0.3",
    "zod": "^3.22.4"
  }
}
